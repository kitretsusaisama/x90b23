{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/core": ["foundation/core"],
      "@/lib": ["foundation/lib"],
      "@/root": ["foundation/root"],
      "@/scripts": ["foundation/scripts"],
      "header/*": ["foundation/core/component/modules/app/primary/header/*"],
      "footer/*": ["foundation/core/component/modules/app/primary/footer/*"],
      "otherextra/*": ["foundation/core/component/modules/app/primary/other_extra/*"],
      "layouts/*": ["foundation/core/component/modules/app/layouts/*"],

    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "foundation/core/component/modules/app/primary/header/primary.jsx", "pages/_document.js", "pages/_document.js", "pages/_document.js"],
  "exclude": ["node_modules"]
}
