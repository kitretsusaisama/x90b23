{
  "name": "quac-private-repo",
  "description": "best of the rest",
  "version": "0.5.0",
  "private": true,
  "author": "Quac Network <hey@quacthemes.com> (@quackery)",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "cross-env ANALYZE=true next build",
   "lint": "next lint --fix --dir pages --dir components --dir lib",
    "type-check": "tsc",
    "prepare": "husky install",
    "commit": "cz"
  },
  "dependencies": {
    "date-fns": "^2.28.0",
    "gray-matter": "^4.0.3",
    "next": "^12.1.0",
    "next-mdx-remote": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/node": "17.0.21",
    "@types/react": "17.0.40",
    "typescript": "4.6.2"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
    },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
